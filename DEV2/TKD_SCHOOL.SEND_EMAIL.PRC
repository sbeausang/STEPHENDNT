CREATE OR REPLACE PROCEDURE TKD_SCHOOL.SEND_EMAIL(ADDRESSEE IN VARCHAR2, MESSAGE IN VARCHAR2) IS
tmpVar NUMBER;
BEGIN
 /* test 82*/
   tmpVar := 0;
   DBMS_OUTPUT.PUT_LINE('TO ' || ADDRESSEE || ': ' || MESSAGE);
   EXCEPTION
     WHEN NO_DATA_FOUND THEN
       NULL;
     WHEN OTHERS THEN
       NULL;
END SEND_EMAIL;
/
